{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../",
  "modulePaths": [
    "<rootDir>"
  ],
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1",
    "tests/(.*)": "<rootDir>/test/$1"
  },
  "testEnvironment": "node",
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}